{
  "manifest_version": 2,
  "name": "SOCKSy",
  "version": "0.1",
  "icons": {
    "16": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab", "declarativeContent", "storage", "tabs"
  ],
  "browser_action": {
    "default_title": "SOCKSy",
    "default_popup": "index.html"
  }
}